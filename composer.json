{
  "name": "phelixjuma/php-enqueue",
  "description": "This is a simple but robust implementation of redis-based job queues in PHP.",
  "type": "library",
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "vlucas/phpdotenv": "^5.3",
    "predis/predis": "^1.1",
    "psr/log":  "^3.0",
    "monolog/monolog": "^3.4",
    "doctrine/annotations": "^2.0",
    "pda/pheanstalk": "^5.0",
    "symfony/console": "^6.0",
    "webmozart/assert": "^1.11"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "autoload": {
    "psr-4": {
      "Phelixjuma\\Enqueue\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phelixjuma\\Enqueue\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "bin": [
    "bin/worker",
    "bin/manager"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Phelix Juma",
      "email": "jp@docusift.ai",
      "homepage": "https://docusift.ai",
      "role": "Developer"
    }
  ]
}
