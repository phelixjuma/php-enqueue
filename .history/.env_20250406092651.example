# AWS ValKey/ElastiCache Configuration
# Replace these values with your actual ValKey configuration
AWS_VALKEY_ENDPOINT="your-elasticache-endpoint.cache.amazonaws.com"
AWS_VALKEY_USERNAME="your-rbac-username"
AWS_VALKEY_PASSWORD="your-rbac-password"
AWS_VALKEY_PORT=6379

# Local Redis Configuration
# Default values for local development
LOCAL_REDIS_HOST="127.0.0.1"
LOCAL_REDIS_PORT=6379
LOCAL_REDIS_PASSWORD=""

# Test Configuration
# Set to "true" to run ValKey tests, "false" to skip
RUN_VALKEY_TESTS=false 